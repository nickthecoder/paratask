{
    "comments": "Used by PlacesTool.\nRow must define isURL() and url\n",
    "rowClassName": "uk.co.nickthecoder.paratask.util.Resource",
    "toolClassName": "uk.co.nickthecoder.paratask.Tool",
    "rowFilterScript": "row.isURL()",
    "includes": [
        
    ],
    "options": [
        {
            "type": "groovy",
            "script": "desktop.browse( row.url )",
            "code": ".",
            "label": "Open in Web Browser",
            "isRow": true,
            "isMultiple": false,
            "prompt": false,
            "newTab": false,
            "refresh": false,
            "aliases": [
                "o"
            ]
        },
        {
            "type": "task",
            "task": "uk.co.nickthecoder.paratask.tools.WebTool",
            "code": "web",
            "label": "Open with built-in Browser",
            "isRow": true,
            "isMultiple": false,
            "prompt": false,
            "newTab": false,
            "refresh": false,
            "parameters": [
                {
                    "name": "address",
                    "expression": "row.url.toString()"
                }
            ]
        },
        {
            "type": "task",
            "task": "uk.co.nickthecoder.paratask.tools.CommandTask",
            "code": "pri",
            "label": "Open new Private Firefox Profile",
            "isRow": true,
            "isMultiple": false,
            "prompt": false,
            "newTab": false,
            "refresh": false,
            "parameters": [
                {
                    "name": "program",
                    "value": "firefox"
                },
                {
                    "name": "arguments",
                    "values": [
                        {
                            "value": "-P"
                        },
                        {
                            "value": "private"
                        },
                        {
                            "expression": "row.url"
                        }
                    ]
                },
                {
                    "name": "directory",
                    "value": ""
                },
                {
                    "name": "output",
                    "value": "INHERRIT"
                }
            ]
        }
    ]
}