# Generated by : paratask generateCompletion --commandName paratask
# Copy to /etc/bash_completion.d/paratask for system wide tab-completion of paratask commands.

_parataskFileComplete()
{
    complete -F _parataskComplete paratask
    return 0
}

_parataskComplete()
{
    local taskName cur prev
    COMPREPLY=()
    _get_comp_words_by_ref cur prev
    if [ "${COMP_CWORD}" == 1 ]
    then
        COMPREPLY=( $( compgen -W 'codeHeader command copyFiles createDirectory customToolList directory editor emptyTrash find flipImage generateCompletion git gitCommit gitCommittedFiles gitLog gitRM gitStash gitStatus grep groovy home imageViewer locate mount moveFiles moveToTrash mythRecorded options optionsFiles places placesDirectory placesList processes project python renameFile resizeImage restoreFiles rotateImage searchAndReplace shortcuts ssh terminal trash web' -- $cur ) )
    else
        taskName=${COMP_WORDS[1]}
        case $taskName in
            codeHeader)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --directories)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directories --extensions --showProcessedFiles --depth --headerTest --withinLines --headerText' -- $cur) )
                    return 0
                fi
                ;;

            command)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --output)
                        COMPREPLY=(INHERRIT WINDOW IGNORE)
                        return 0
                        ;;
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--program --arguments --directory --output' -- $cur) )
                    return 0
                fi
                ;;

            copyFiles)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --files)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                    --toDirectory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--files --toDirectory' -- $cur) )
                    return 0
                fi
                ;;

            createDirectory)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --parentDirectory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--parentDirectory --directoryName' -- $cur) )
                    return 0
                fi
                ;;

            customToolList)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --toolbar)
                        COMPREPLY=(null TOP BOTTOM LEFT RIGHT)
                        return 0
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--toolbar --tools' -- $cur) )
                    return 0
                fi
                ;;

            directory)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --placesFile)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                    --treeRoot|--directories)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directories --treeRoot --placesFile --onlyFiles --extensions --includeHidden --foldSingleDirectories --thumbnailHeight --directoryThumbnail --autoRefresh' -- $cur) )
                    return 0
                fi
                ;;

            editor)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --file)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--file --ignoreFileSizeCheck --initialText --goToLine --findText --matchCase --useRegex' -- $cur) )
                    return 0
                fi
                ;;

            emptyTrash)
                if [[ "$prev" == --* ]] ; then
                    # No choice or file parameters
                    COMPREPLY=()
                else
                    COMPREPLY=( $( compgen -W '--daysAgo' -- $cur) )
                    return 0
                fi
                ;;

            find)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --matchType)
                        COMPREPLY=(GLOB_CASE_SENSITIVE GLOB_CASE_INSENSITIVE REGEX_CASE_SENSITIVE REGEX_CASE_INSENSITIVE)
                        return 0
                        ;;
                    --type)
                        COMPREPLY=(any f d l s p b c)
                        return 0
                        ;;
                    --newerThanFile)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directory --filename --matchType --wholeName --user --group --type --typeFollowsSymlink --emptyFiles --newerThanFile --followSymlinks --otherFileSystems --minDepth --maxDepth --thumbnailHeight' -- $cur) )
                    return 0
                fi
                ;;

            flipImage)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --direction)
                        COMPREPLY=(x y)
                        return 0
                        ;;
                    --output)
                        COMPREPLY=(INHERRIT WINDOW IGNORE)
                        return 0
                        ;;
                    --inputFile|--outputFile)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--inputFile --direction --outputFile --output' -- $cur) )
                    return 0
                fi
                ;;

            generateCompletion)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --output)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--commandName --output' -- $cur) )
                    return 0
                fi
                ;;

            git)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --grepType)
                        COMPREPLY=(regular extended fixed perl)
                        return 0
                        ;;
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directory --maxItems --grep --grepType --merges --matchCase --since --until' -- $cur) )
                    return 0
                fi
                ;;

            gitCommit)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --output)
                        COMPREPLY=(INHERRIT WINDOW IGNORE)
                        return 0
                        ;;
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--message --all --directory --output' -- $cur) )
                    return 0
                fi
                ;;

            gitCommittedFiles)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directory --commit --compareTo' -- $cur) )
                    return 0
                fi
                ;;

            gitLog)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --grepType)
                        COMPREPLY=(regular extended fixed perl)
                        return 0
                        ;;
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directory --maxItems --grep --grepType --merges --matchCase --since --until' -- $cur) )
                    return 0
                fi
                ;;

            gitRM)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --option)
                        COMPREPLY=(none -f --cache)
                        return 0
                        ;;
                    --files)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directory --files --option' -- $cur) )
                    return 0
                fi
                ;;

            gitStash)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directory' -- $cur) )
                    return 0
                fi
                ;;

            gitStatus)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directory' -- $cur) )
                    return 0
                fi
                ;;

            grep)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --type)
                        COMPREPLY=(regular extended fixed perl)
                        return 0
                        ;;
                    --part)
                        COMPREPLY=(any word line)
                        return 0
                        ;;
                    --output)
                        COMPREPLY=(INHERRIT WINDOW IGNORE)
                        return 0
                        ;;
                    --filesOrDirectories)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--filesOrDirectories --patterns --matchCase --type --part --invertResults --followSymLinks --maxMatches --contextLines --additionalOptions --output' -- $cur) )
                    return 0
                fi
                ;;

            groovy)
                if [[ "$prev" == --* ]] ; then
                    # No choice or file parameters
                    COMPREPLY=()
                else
                    COMPREPLY=( $( compgen -W '' -- $cur) )
                    return 0
                fi
                ;;

            home)
                if [[ "$prev" == --* ]] ; then
                    # No choice or file parameters
                    COMPREPLY=()
                else
                    COMPREPLY=( $( compgen -W '' -- $cur) )
                    return 0
                fi
                ;;

            imageViewer)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --file)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--file' -- $cur) )
                    return 0
                fi
                ;;

            locate)
                if [[ "$prev" == --* ]] ; then
                    # No choice or file parameters
                    COMPREPLY=()
                else
                    COMPREPLY=( $( compgen -W '--pattern --matchWholePath --checkFileExists --regularExpressions --caseSensitive --maxItems' -- $cur) )
                    return 0
                fi
                ;;

            mount)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --toolbar)
                        COMPREPLY=(null TOP BOTTOM LEFT RIGHT)
                        return 0
                        ;;
                    --buttonText)
                        COMPREPLY=(DIRECTORY_PATH DIRECTORY_NAME LABEL)
                        return 0
                        ;;
                    --excludeMountPoints)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--toolbar --buttonText --excludeFSTypes --excludeMountPoints' -- $cur) )
                    return 0
                fi
                ;;

            moveFiles)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --files)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                    --toDirectory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--files --toDirectory' -- $cur) )
                    return 0
                fi
                ;;

            moveToTrash)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --files)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--files' -- $cur) )
                    return 0
                fi
                ;;

            mythRecorded)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--server --database --user --password --directory' -- $cur) )
                    return 0
                fi
                ;;

            options)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --directory)
                        COMPREPLY=(/home/nick/projects/paratask/build/install/paratask/options /home/nick/.config/paratask/options)
                        return 0
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--optionsName --directory' -- $cur) )
                    return 0
                fi
                ;;

            optionsFiles)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --directory)
                        COMPREPLY=( /home/nick/projects/paratask/build/install/paratask/options /home/nick/.config/paratask/options)
                        return 0
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directory' -- $cur) )
                    return 0
                fi
                ;;

            places)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --toolbar)
                        COMPREPLY=(null TOP BOTTOM LEFT RIGHT)
                        return 0
                        ;;
                    --files)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--files --toolbar' -- $cur) )
                    return 0
                fi
                ;;

            placesDirectory)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --file)
                        COMPREPLY=(Computer Gidea Gnome Extensions Gnome Places goko Home paratask People Projects Server Home Settings TV Shows)
                        return 0
                        ;;
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directory --file' -- $cur) )
                    return 0
                fi
                ;;

            placesList)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --toolbar)
                        COMPREPLY=(null TOP BOTTOM LEFT RIGHT)
                        return 0
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--toolbar --files' -- $cur) )
                    return 0
                fi
                ;;

            processes)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --filter_choice)
                        COMPREPLY=()
                        return 0
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--filter --filter_choice --command --user --group --pids' -- $cur) )
                    return 0
                fi
                ;;

            project)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --projectFiles)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                    --directory|--optionsPath)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--directory --projects --projectFiles --optionsPath' -- $cur) )
                    return 0
                fi
                ;;

            python)
                if [[ "$prev" == --* ]] ; then
                    # No choice or file parameters
                    COMPREPLY=()
                else
                    COMPREPLY=( $( compgen -W '--version' -- $cur) )
                    return 0
                fi
                ;;

            renameFile)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --file)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--file --newName' -- $cur) )
                    return 0
                fi
                ;;

            resizeImage)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --resizeOptions_choice)
                        COMPREPLY=()
                        return 0
                        ;;
                    --output)
                        COMPREPLY=(INHERRIT WINDOW IGNORE)
                        return 0
                        ;;
                    --inputFile|--outputFile)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--inputFile --resizeOptions --resizeOptions_choice --width --height --keepAspectRatio --percentage --outputFile --output' -- $cur) )
                    return 0
                fi
                ;;

            restoreFiles)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --files)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--files' -- $cur) )
                    return 0
                fi
                ;;

            rotateImage)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --rotation)
                        COMPREPLY=(0 90 180 -90)
                        return 0
                        ;;
                    --output)
                        COMPREPLY=(INHERRIT WINDOW IGNORE)
                        return 0
                        ;;
                    --inputFile|--outputFile)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--inputFile --rotation --outputFile --output' -- $cur) )
                    return 0
                fi
                ;;

            searchAndReplace)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --files)
                        complete -F _parataskFileComplete -o filenames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--files --searchString --replaceString --useRegex --matchCase' -- $cur) )
                    return 0
                fi
                ;;

            shortcuts)
                if [[ "$prev" == --* ]] ; then
                    # No choice or file parameters
                    COMPREPLY=()
                else
                    COMPREPLY=( $( compgen -W '' -- $cur) )
                    return 0
                fi
                ;;

            ssh)
                if [[ "$prev" == --* ]] ; then
                    # No choice or file parameters
                    COMPREPLY=()
                else
                    COMPREPLY=( $( compgen -W '--user --host --xForwarding --command' -- $cur) )
                    return 0
                fi
                ;;

            terminal)
                if [[ "$prev" == --* ]] ; then
                case $prev in
                    --directory)
                        complete -F _parataskFileComplete -o dirnames paratask
                        return 124
                        ;;
                esac
                else
                    COMPREPLY=( $( compgen -W '--program --arguments --directory --title --closeWhenFinished' -- $cur) )
                    return 0
                fi
                ;;

            trash)
                if [[ "$prev" == --* ]] ; then
                    # No choice or file parameters
                    COMPREPLY=()
                else
                    COMPREPLY=( $( compgen -W '' -- $cur) )
                    return 0
                fi
                ;;

            web)
                if [[ "$prev" == --* ]] ; then
                    # No choice or file parameters
                    COMPREPLY=()
                else
                    COMPREPLY=( $( compgen -W '--address' -- $cur) )
                    return 0
                fi
                ;;

        esac
        
    fi
}

complete -F _parataskComplete paratask
